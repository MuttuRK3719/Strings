   public static void purmulation(String p,String up){
        if(up.isEmpty()){
            System.out.println(p);
            return;
        }
        char ch=up.charAt(0);
        for(int i=0;i<=p.length();i++){
            String start=p.substring(0,i);
            String f=p.substring(i,p.length());
            purmulation(start+ch+f,up.substring(1));
        }
    }
